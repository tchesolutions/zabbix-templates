<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2022-01-28T14:21:45Z</date>
    <groups>
        <group>
            <name>Raisecom</name>
        </group>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>OLT Raisecom</template>
            <name>OLT Raisecom</name>
            <templates>
                <template>
                    <name>Template Module Interfaces SNMPv2</name>
                </template>
            </templates>
            <groups>
                <group>
                    <name>Raisecom</name>
                </group>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Chassi</name>
                </application>
                <application>
                    <name>CPU</name>
                </application>
                <application>
                    <name>OLT</name>
                </application>
                <application>
                    <name>ONU</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>CPU</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.8886.18.1.7.1.1.1.4.1.0</snmp_oid>
                    <key>sysCPU</key>
                    <delay>30s</delay>
                    <units>%</units>
                    <applications>
                        <application>
                            <name>CPU</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last(5m)}&gt;80</expression>
                            <name>Alto uso de CPU em {HOST.NAME}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Versão de Hardware</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.8886.1.27.2.1.1.4.0</snmp_oid>
                    <key>sysHardware</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>HARDWARE_FULL</inventory_link>
                    <applications>
                        <application>
                            <name>Chassi</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>MAC Address</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>.1.3.6.1.4.1.8886.1.27.2.1.1.15.0</snmp_oid>
                    <key>sysMAC</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>MACADDRESS_A</inventory_link>
                    <applications>
                        <application>
                            <name>Chassi</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Modelo</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.8886.1.27.2.1.1.6.0</snmp_oid>
                    <key>sysModel</key>
                    <delay>1d</delay>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <inventory_link>MODEL</inventory_link>
                    <applications>
                        <application>
                            <name>Chassi</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Temperatura OLT</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>1.3.6.1.4.1.8886.1.27.2.1.1.10.0</snmp_oid>
                    <key>sysTemperature</key>
                    <delay>60</delay>
                    <history>7d</history>
                    <trends>30d</trends>
                    <units>ºC</units>
                    <applications>
                        <application>
                            <name>Chassi</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last(#1)}&gt;60 and {last(#2)}&gt;60</expression>
                            <name>Temperatura alta em {HOST.NAME}</name>
                            <priority>HIGH</priority>
                        </trigger>
                    </triggers>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>ONU</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#ONU},1.3.6.1.4.1.8886.18.3.6.3.1.1.1]</snmp_oid>
                    <key>MacAddressOnu</key>
                    <delay>10m</delay>
                    <lifetime>7d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Rx da ONU: {#ONU}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.8886.18.3.6.3.1.1.16.{#SNMPINDEX}</snmp_oid>
                            <key>ONURx[{#SNMPINDEX}]</key>
                            <delay>3m</delay>
                            <history>7d</history>
                            <trends>30d</trends>
                            <value_type>FLOAT</value_type>
                            <units>dBm</units>
                            <applications>
                                <application>
                                    <name>ONU</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JAVASCRIPT</type>
                                    <params>value = value - 15000
value = value/500
return value
</params>
                                </step>
                            </preprocessing>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
                <discovery_rule>
                    <name>PON</name>
                    <type>SNMP_AGENT</type>
                    <snmp_oid>discovery[{#PON},.1.3.6.1.2.1.2.2.1.2]</snmp_oid>
                    <key>oltPonDesc</key>
                    <delay>3600</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#PON}</macro>
                                <value>^gpon-olt</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <item_prototypes>
                        <item_prototype>
                            <name>ONU NA PON {#PON}</name>
                            <type>SNMP_AGENT</type>
                            <snmp_oid>1.3.6.1.4.1.8886.18.3.1.2.1.1.3.{#SNMPINDEX}</snmp_oid>
                            <key>oltPonAuthOnuNum[{#PON}]</key>
                            <history>7d</history>
                            <trends>30d</trends>
                            <applications>
                                <application>
                                    <name>OLT</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
</zabbix_export>
